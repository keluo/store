/* component/select/select.wxss */
.com-selectBox{
  display:inline-block;
  position: relative;
}
.com-sContent{
    background-color: #fff;
    font-size: 32rpx;
    position: relative;
    height: 48rpx;
    line-height: 48rpx;
    text-align: right;
    display: flex;
}
.icon-bottom {
  background: url(data:image/png;base64,) no-repeat center 50%;
  background-size: 23rpx 13rpx;
  width: 48rpx;
  height: 48rpx;
}
.com-sTxt{
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
  padding:0 6rpx 0 6rpx;
  font-size: 32rpx;
  color: #108EE9;
}
.com-sList{
  background:#fff;
  width:inherit;
  position:absolute;
  /*border:1rpx solid #e2e2e2;*/
  border-top:none;
  box-sizing:border-box;
  z-index:100;
  right:0rpx;
  border-radius:4rpx;
  top:65rpx;
  min-width:250rpx;
  min-height:20rpx;
}
.com-sList:after {
  content: " ";
  position: absolute;
  width:0px;
  height:0px;
  border-top: 0rpx solid transparent;
  border-right:15rpx solid transparent;
  border-bottom: 16rpx solid #fff;
  border-left: 15rpx solid transparent;
  top: -15rpx;
  right: 60rpx;
}
.left .com-sList{
  left:0rpx;
  right: auto;
}
.left .com-sList:after{
  top: -15rpx;
  left: 60rpx;
  right: auto;
}
.com-sItem{
  padding:18rpx 23rpx;
  text-align:left;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
  font-size:28rpx;
  min-width:250rpx;
  position: relative;
}
.com-sItem:after{
  content: " ";
  position: absolute;
  bottom: 0rpx;
  right: 23rpx;
  left: 23rpx;
  height: 0rpx;
  border-bottom: 1rpx solid #eeeeee;
}
.com-sItem.actived {
  background:#f5f2f0;
}
.com-sItem:first-child{
    border-top: none;
}
.com-bg {
  position:fixed;
  height:100%;
  width:100%;
  background-color:#000;
  top:0rpx;
  left:0rpx;
  z-index:99;
  padding:1000rpx 0;
  margin-top:-500rpx;
  overflow:hidden;
  opacity:0.65;
}