/**app.wxss**/
page{
  background-color: #f5f5f5;
  color: #1A1A1A;
}
.container {
  height: 100%;
  width: 100%;
}
.green {
  color: #00a65a;
}
.grey {
  color: #666;
}
.blue {
  color: #0812FF;
}
.btn {
  border-radius:2px;
  display:inline-block;
  height: 94rpx;
  line-height: 94rpx;
  font-size:32rpx;
  background-color:#108EE9;
  color:#fff;
  border:0px;
  cursor:pointer;
  box-sizing: border-box;
}
.btn.btn-white {
  background-color: #fff;
  border: 1rpx solid #090AE8;
  color: #090AE8;
}
.btn.btn-submit{
  width:100%;
  display:block;
}
.btn[disabled]{
  background-color: #090AE8 !important; 
  opacity: 0.3;
  color: #fff !important;
}
