/* 占比 */

.overview-box {
  display: flex;
  justify-content: center;
  padding: 22rpx 0 32rpx 0;
  background: #fff;
}

.overview-box .overview-item {
  width: 228rpx;
  text-align: center;
}

.overview-box .overview-item .overview-title {
  color: #333;
  font-size: 28rpx;
}

.overview-box .overview-item .overview-num {
  color: #333;
  margin-top: 6rpx;
  font-size: 48rpx;
  height: 56rpx;
  font-weight: 500;
  line-height: 56rpx;
}

.overview-box .overview-item .overview-num .overview-unit {
  display: inline;
  color: #999;
  font-size: 24rpx;
}

.overview-box .overview-item .overview-RingRatio {
  color: #999;
  font-size: 28rpx;
  margin-top: 12rpx;
}

/*  */

.item-box {
  background: #fff;
  margin-top: 32rpx;
}

.item-box .item-title {
  border-bottom: 1px solid #eee;
  padding: 28rpx 32rpx;
  display: flex;
  justify-content: space-between;
}

.item-box .compared {
  color: #108ee9;
  font-size: 34rpx;
}

.item-box .item-title .item-title-txt {
  color: #333;
  font-size: 34rpx;
  border-left: 12rpx solid #108ee9;
  padding-left: 20rpx;
}

.item-box .item-title .item-title-prompt {
  color: #999;
  font-size: 28rpx;
}

.item-box .item-box-content {
  padding: 0 32rpx 32rpx 32rpx;
}

/* 选项卡 */

.tab-box {
  height: 64rpx;
  padding: 32rpx 0 12rpx 0;
  background: #fff;
  border-radius: 4rpx;
  display: flex;
  justify-content: center;
}

.tab-box .tab-item {
  text-align: center;
  width: 160rpx;
  line-height: 64rpx;
  color: #108ee9;
  background: #fff;
  font-size: 28rpx;
  border: 1px solid #108ee9;
  box-sizing: border-box;
}

.tab-box .tab-item:first-child {
  border-radius: 4rpx 0 0 4rpx;
  border-right: 0;
}

.tab-box .tab-item:last-child {
  border-radius: 0 4rpx 4rpx 0;
  border-left: 0;
}

.tab-box .tab-item.active {
  background: #108ee9;
  color: #fff;
  /* border-left: 1px solid #108EE9; */
}

/* 增加图标 */

.icon-increase {
  display: inline-block;
  border: 18rpx solid #52c41a;
  border-left: 10rpx solid transparent;
  border-right: 10rpx solid transparent;
  border-top: 2rpx solid transparent;
}

/* 下降图标 */

.icon-decline {
  display: inline-block;
  border: 18rpx solid #f4222c;
  border-left: 10rpx solid transparent;
  border-right: 10rpx solid transparent;
  border-bottom: 2rpx solid transparent;
}

/* 问号 */

.icon-question {
  display: inline-block;
  width: 28rpx;
  height: 28rpx;
  background: url('data:image/png;base64,');
  background-size: 100%;
  position: relative;
  top: 4rpx;
}

/* 日期 */

.icon-time {
  display: inline-block;
  width: 44rpx;
  height: 50rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100%;
  vertical-align: middle;
}

/* 下载 */

.icon-download {
  display: inline-block;
  width: 44rpx;
  height: 44rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100%;
  vertical-align: middle;
}

/* 弹出层 */
.pop-up {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.7);
  z-index: 999999;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.pop-up-container {
  width: 540rpx;
  background-color: #fff;
  border-radius: 4rpx;
}
.pop-title {
  color: #333;
  font-size: 36rpx;
  font-weight: bold;
  text-align: center;
  padding: 48rpx 0 16rpx 0;
}
.pop-content {
  color: #333;
  font-size: 28rpx;
  text-align: center;
  padding: 0 20rpx;
  white-space: normal;
}
.pop-foot {
  padding: 26rpx 0;
  text-align: center;
  margin-top: 32rpx;
  border-top: 1px solid #ddd;
  color: #108ee9;
  font-size: 36rpx;
}

/* 隐藏 */

.hidden {
  z-index: 1;
  display: block;
  position: absolute;
  top: 84rpx;
  transform: translateX(-100%);
}
