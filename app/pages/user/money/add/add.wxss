/* pages/user/money/add/add.wxss */
.item-charge {
  margin-bottom: 0rpx;
  border-top: 1rpx solid #eee;
  border-bottom: 1rpx solid #eee;
}
.item-money {
  padding: 28rpx 32rpx;
}
.item-money .money-txt {
  font-size: 34rpx;
  line-height: 48rpx;
  color: #333;
}
.item-money .money-num {
  font-size: 60rpx;
  line-height: 100rpx;
  display: flex;
  margin-top: 30rpx;
}
.item-money .money-num input{
  font-size: 96rpx;
  height: 100rpx;
  margin-left: 10rpx;
}
.item-charge .charge-method {
  display: flex;
}
.item-charge .icon-wx {
  width: 38rpx;
  height: 38rpx;
  background: url(data:image/png;base64,) no-repeat center 50%;
  background-size: 32rpx 29rpx;
  background-position: 0rpx 6rpx;
  margin-right: 4rpx;
}